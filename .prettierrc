{
  "semi": true,
  "singleQuote": false,
  "arrowParens": "always",
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "overrides": [
    {
      "files": ["*.js", "*.jsx"],
      "options": {
        "semi": true,
        "singleQuote": false,
        "arrowParens": "always",
        "trailingComma": "all",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": false
      }
    }
  ],
  "plugins": ["prettier-plugin-organize-imports"]
}
